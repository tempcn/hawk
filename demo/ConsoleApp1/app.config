<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net "/>
  </configSections>
  <connectionStrings>
    <add name="redis" connectionString="127.0.0.1:4963,password=IAmWoker!520LoverForever*(annotation)LinuxWindowsMac,ssl=false" />
  </connectionStrings>

  <log4net>
    <root>
      <!--OFF < FATAL < ERROR < WARN < INFO < DEBUG < ALL-->
      <level value="ALL"/>
      <appender-ref ref="InfoFileAppender"/>
    </root>
    <appender name="InfoFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="z:\log\test.log" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <datePattern value="yyyyMMdd" />
      <!--最多产生的日志文件数，超过则只保留最新的n个。设定值value="－1"为不限文件数-->
      <maxSizeRollBackups value="100" />
      <maximumFileSize value="1MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout" >
        <param name="ConversionPattern" value="%date [%p] (%file:%line) [%logger] - %message%newline" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="INFO" />
        <levelMax value="WARN" />
      </filter>
    </appender>
  </log4net>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
  </startup>
</configuration>
